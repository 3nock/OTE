QFrame[frameShape="4"] {
    border: none;
    background: rgb(35,35,35);
}

QFrame[frameShape="5"] {
    border: none;
    background: transparent;
}

QFrame#frame, QFrame#frame1, QFrame#frame_newSpecialWordlist, QFrame#frame_nameservers{
    background-color: rgb(45, 45, 45);
}

