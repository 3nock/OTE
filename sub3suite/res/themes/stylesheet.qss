QScrollBar:vertical {
    background-color: transparent;
    width: 10px;
}

QScrollBar:horizontal {
    background-color: transparent;
    height: 10px;
}

QScrollBar::handle:vertical {
    min-height: 17px;
    background-color: rgb(61,61,61);
}

QScrollBar:handle:horizontal {
    min-width: 17px;
    background-color: rgb(61,61,61);
}

QScrollBar::handle:vertical:hover {
    background-color: rgb(80,80,80);
}

QScrollBar:handle:horizontal:hover {
    background-color: rgb(80,80,80);
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: transparent;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: transparent;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0px;
    width: 0px;
}

QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
    height: 0px;
    width: 0px;
}

QStatusBar {
    border-style: solid;
    border-style: outset;
    border-width: 1px;
    border-top-color: rgb(110,110,110);
    border-right-color: rgb(110,110,110);
    border-left-color: rgb(110,110,110);
    border-bottom-color: transparent;
}

QToolButton:!pressed {
    min-height: 13px;
    border-style: outset;
    border-color: rgb(30,30,30);
    border-width: 1px;
    border-style: solid;
    padding: 2px;
    color: rgba(218, 218, 163, 250);
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,stop:0 rgba(70, 70, 70, 255),stop: 0.4 rgba(45, 45, 45, 255), stop: 0.5 rgba(45, 45, 45, 255) stop:1 rgba(70, 70, 70, 255));
}

QToolButton:hover {
    border-style: outset;
    border-color: rgb(30,30,30);
    border-width: 1px;
    border-style: solid;
    padding: 2px;
    color: rgba(218, 218, 163, 250);
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,stop:0 rgba(75, 75, 75, 255),stop: 0.4 rgba(50, 50, 50, 255), stop: 0.5 rgba(50, 50, 50, 255) stop:1 rgba(75, 75, 75, 255));
}

QToolButton:pressed {
    border-style: outset;
    border-color: rgb(30,30,30);
    border-width: 1px;
    border-style: solid;
    padding: 2px;
    color: rgba(220, 220, 165, 250);
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,stop:0 rgba(80, 80, 80, 255),stop: 0.4 rgba(55, 55, 55, 255), stop: 0.5 rgba(55, 55, 55, 255) stop:1 rgba(80, 80, 80, 255));
}

QSpinBox {
    border-style: inset;
    border-width: 1px;
    border-color: black;
    border-style: solid;
    color: rgb(250,250,250);
    background-color: rgba(65, 65, 65, 255);
}

QComboBox {
    border-style: outset;
    border-style: solid;
    border-color: black;
    border-width: 1px;
    color: rgb(230,230,230);
    padding-left: 5px;
    padding-bottom: 1px;
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(45,45,45,255), stop:1 rgba(52,52,52,255));
}

QComboBox QWidget {
    border-style: outset;
    border-style: solid;
    border-color: black;
    border-width: 1px;
    color: rgb(250,250,250);
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(45,45,45,255), stop:1 rgba(52,52,52,255));
    selection-background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(57, 57, 57, 255), stop:1 rgba(141, 141, 106, 180));
}

QComboBox::drop-down {
    image: url(:/img/res/icons/downArrow.png);
    padding-top: 3px;
    padding-right: 5px;
    padding-left: 5px;
}

QComboBox QWidget::item {
    padding-top: 3px;
    padding-bottom: 3px;
}


