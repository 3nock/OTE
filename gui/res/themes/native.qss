    padding: 0 8px;
    color: rgb(255, 255, 255);
    background-color:rgb(42, 42, 42);
}

QProgressBar {
    text-align: center;
    color: white;
    border-width: 1px;
    border-radius: 10px;
    border-color: rgb(0,0,0);
    border-style: inset;
    background-color: rgb(40,40,40);
}

QProgressBar::chunk {
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(77, 77, 77, 255), stop:0.6 rgb(143, 143, 107));
}

QMenuBar {
    background: rgb(62, 62, 62);
}

QMenu {
    min-width: 60px;
    border-style: solid;
    border-color: black;
    border-width: 1px;
    background-color:rgb(62,62,62);
}

QMenuBar::item {
    color:white;
    spacing: 15px;
    padding-left: 8px;
    padding-right: 8px;
    padding-top: 1px;
    padding-bottom: 1px;
    background: transparent;
}

QMenuBar::item:selected, QMenuBar::item:hover{
    background-color: rgb(135, 135, 100);
    color: black;
}

QMenu::item:selected {
    color:rgb(0,0,0);
    border-style: solid;
    border-color: black;
    background-color: rgb(141,141,106);
}

QMenu::​separator {
    background-color: rgb(0, 0, 0);
}

QMenu::item {
    color:rgb(230,230,230);
    background-color:rgb(62,62,62);
    padding-left:20px;
    padding-top:4px;
    padding-bottom:4px;
    padding-right:20px;
}

QTabWidget::tab-bar {
    left: 10px;
}

QTabWidget::pane {
    border-color: grey;
    background-color:rgb(62,62,62);
    border-style: solid;
    border-top-width: 1px;
    border-right-width: 1px;
    border-left-width: 1px;
    border-bottom-width: 0px;
}

QTabBar::tab {
    min-height: 16px;
    min-width: 55px;
    padding: 1px;
    color: white;
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(47, 47, 47, 255), stop:1 rgba(67, 67, 67, 255));
    border-style: solid;
    border-width: 1px;
    border-color: grey;
}

QTabBar::tab:hover {
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(57, 57, 57, 255), stop:1 rgb(141, 141, 106));
    margin-top: 0px;
    margin-left: 0px;
    margin-right: 1px;
}

QTabBar::tab:selected {
    background-color: grey;
    color: rgb(37,37,37);
    margin-top: 0px;
    margin-left: 0px;
    margin-right: 1px;
}

QtabBar::tab:!selected {
    margin-top: 1px;
    margin-right: 1px;
}

QCheckBox {
    color: rgb(230,230,230);
