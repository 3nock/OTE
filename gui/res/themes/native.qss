}

QCheckBox::indicator:checked {
    border-style:solid;
    border-width:1px;
    border-color: rgb(47,47,47);
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(75, 75, 75, 255), stop:1 rgb(143, 143, 107));
}

QCheckBox::indicator:unchecked {
