QFrame[frameShape="4"] {
    border: none;
    background: rgb(40,40,40);
}

QFrame[frameShape="5"] {
    border: none;
    background: transparent;
}

QFrame#frame, QFrame#frame_newSpecialWordlist, QFrame#frame_nameservers {
    background-color: rgb(55, 55, 55);
}

