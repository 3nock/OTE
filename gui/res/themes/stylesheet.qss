QScrollBar:vertical {
    background-color: transparent;
    width: 10px;
}

QScrollBar:horizontal {
    background-color: transparent;
    height: 10px;
}

QScrollBar::handle:vertical {
    min-height: 17px;
    background-color: rgb(81,81,81);
}

QScrollBar:handle:horizontal {
    min-width: 17px;
    background-color: rgb(81,81,81);
}

QScrollBar::handle:vertical:hover {
    background-color: rgb(100,100,100);
}

QScrollBar:handle:horizontal:hover {
    background-color: rgb(100,100,100);
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: transparent;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: transparent;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0px;
    width: 0px;
}

QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
    height: 0px;
    width: 0px;
}

QStatusBar {
    border-style: solid;
    border-style: outset;
    border-width: 1px;
    border-top-color: rgb(110,110,110);
    border-right-color: rgb(110,110,110);
    border-left-color: rgb(110,110,110);
    border-bottom-color: transparent;
}

QToolButton:!pressed {
    border-style: outset;
    border-width: 2px;
    border-top-color: qlineargradient(spread:pad, x1:0.5, y1:0.6, x2:0.5, y2:0.4, stop:0 rgba(140, 140, 140, 255), stop:1 rgba(0,0,0, 255));
    border-right-color: qlineargradient(spread:pad, x1:0.4, y1:0.5, x2:0.6, y2:0.5, stop:0 rgba(140, 140, 140, 255), stop:1 rgba(0,0,0,255));
    border-left-color: qlineargradient(spread:pad, x1:0.6, y1:0.5, x2:0.4, y2:0.5, stop:0 rgba(140, 140, 140, 255), stop:1 rgba(0,0,0,255));
    border-bottom-color: rgb(105, 105, 105);
    border-bottom-width: 1px;
    border-style: solid;
    border-radius: 15px;
    color: rgba(218, 218, 163, 250);
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(60, 60, 60, 255), stop:1 rgba(144, 144, 109, 90));
}

